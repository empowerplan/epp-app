{
  "region-line": {
    "type": "line",
    "paint": {
      "line-color": "rgb(250, 174,29)",
      "line-width": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        3,
        0.25
      ]
    }
  },
  "region-fill": {
    "type": "fill",
    "paint": {
      "fill-color": "rgb(250,174,29)",
      "fill-opacity": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        0,
        0.1
      ]
    }
  },
  "region-label": {
    "type": "symbol",
    "layout": {
      "text-variable-anchor": [
        "center",
        "top"
      ],
      "text-radial-offset": 2,
      "text-field": "{name}",
      "text-letter-spacing": 0.04
    },
    "paint": {
      "text-color": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        "#222222",
        "#000000"
      ],
      "text-halo-color": "hsl(0, 0%, 100%)",
      "text-halo-width": 0.85,
      "text-halo-blur": 1.5
    }
  },
  "results": {
    "type": "fill",
    "paint": {
      "fill-color": [
        "interpolate",
        ["linear"],
        ["feature-state", "value"],
        0.3,
        "#fef0d9",
        0.6,
        "#fdcc8a",
        0.8,
        "#fc8d59",
        0.9,
        "#e34a33",
        1,
        "#b30000"
      ],
      "fill-opacity": 0.8
    }
  }
}
