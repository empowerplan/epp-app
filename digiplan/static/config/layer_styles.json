{
  "region-fill": {
    "paint": {
      "fill-color": "rgb(250,174,29)",
      "fill-opacity": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        0,
        0.1
      ]
    },
    "type": "fill"
  },
  "region-label": {
    "layout": {
      "text-field": "{name}",
      "text-letter-spacing": 0.04,
      "text-radial-offset": 2,
      "text-variable-anchor": [
        "center",
        "top"
      ]
    },
    "paint": {
      "text-color": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        "#222222",
        "#000000"
      ],
      "text-halo-blur": 1.5,
      "text-halo-color": "hsl(0, 0%, 100%)",
      "text-halo-width": 0.85
    },
    "type": "symbol"
  },
  "region-line": {
    "paint": {
      "line-color": "rgb(250, 174,29)",
      "line-width": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        3,
        0.25
      ]
    },
    "type": "line"
  },
  "wind": {
      "type": "symbol",
        "layout": {
          "icon-image": "wind",
          "icon-size": 0.5,
          "icon-allow-overlap": true
        },
        "paint": {
          "icon-color": "blue"
        }
  },
  "pvroof": {
    "type": "symbol",
      "layout": {
        "icon-image": "pvroof",
        "icon-size": 0.5,
        "icon-allow-overlap": true
      },
      "paint": {
        "icon-color": "blue"
      }
  },
  "pvground": {
    "type": "symbol",
      "layout": {
        "icon-image": "pvground",
        "icon-size": 0.5,
        "icon-allow-overlap": true
      },
      "paint": {
        "icon-color": "blue"
      }
  },
  "results": {
    "paint": {
      "fill-color": [
        "interpolate",
        [
          "linear"
        ],
        [
          "feature-state",
          "value"
        ],
        0.3,
        "#fef0d9",
        0.6,
        "#fdcc8a",
        0.8,
        "#fc8d59",
        0.9,
        "#e34a33",
        1,
        "#b30000"
      ],
      "fill-opacity": 0.8
    },
    "type": "fill"
  }
}
