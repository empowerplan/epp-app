{
  "region-fill": {
    "type": "fill",
    "paint": {
      "fill-color": "rgb(250, 174, 29)",
      "fill-opacity": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        0,
        0.1
      ]
    }
  },
  "region-label": {
    "type": "symbol",
    "layout": {
      "text-size": [
        "interpolate",
        ["linear"],
        ["zoom"],
        8,
        10,
        14,
        30
      ],
      "text-field": "{name}",
      "text-letter-spacing": 0.04,
      "text-radial-offset": 2,
      "text-variable-anchor": [
        "center",
        "top"
      ]
    },
    "paint": {
      "text-color": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        "#222222",
        "#000000"
      ],
      "text-halo-blur": 1.5,
      "text-halo-color": "hsl(0, 0%, 100%)",
      "text-halo-width": 0.85
    }
  },
  "region-line": {
    "type": "line",
    "paint": {
      "line-color": "rgb(250, 174, 29)",
      "line-width": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        3,
        0.5
      ]
    }
  },
  "region_boundaries": {
    "type": "line",
    "paint": {
      "line-color": "rgb(250, 174, 29)",
      "line-width": 2
    }
  },
  "wind": {
