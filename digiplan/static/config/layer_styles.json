{
  "region-fill": {
    "type": "fill",
    "paint": {
      "fill-color": "rgb(250,174,29)",
      "fill-opacity": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        0,
        0.1
      ]
    }
  },
  "region-label": {
    "type": "symbol",
    "layout": {
      "text-field": "{name}",
      "text-letter-spacing": 0.04,
      "text-radial-offset": 2,
      "text-variable-anchor": [
        "center",
        "top"
      ]
    },
    "paint": {
      "text-color": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        "#222222",
        "#000000"
      ],
      "text-halo-blur": 1.5,
      "text-halo-color": "hsl(0, 0%, 100%)",
      "text-halo-width": 0.85
    }
  },
  "region-line": {
    "type": "line",
    "paint": {
      "line-color": "rgb(250, 174,29)",
      "line-width": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        3,
        0.25
      ]
    }
  },
  "wind": {
    "type": "symbol",
    "filter": ["!", ["has", "point_count"]],
    "layout": {
      "icon-image": "wind",
      "icon-size": 0.4,
      "icon-allow-overlap": true
    },
    "paint": {
      "icon-color": "blue"
    }
  },
  "wind_cluster": {
    "type": "circle",
    "filter": ["has", "point_count"],
    "paint": {
      "circle-color": "#6A89CC",
      "circle-radius": [
        "step",
        ["get", "point_count"],
        20,
        15,
        30,
        50,
        40
      ]
    }
  },
  "wind_cluster_count": {
    "type": "symbol",
    "filter": ["has", "point_count"],
    "layout": {
      "text-field": "{point_count_abbreviated}",
      "text-font": ["DIN Offc Pro Medium", "Arial Unicode MS Bold"],
      "text-size": 12
    }
  },
