{
  "region-line": {
    "type": "line",
    "paint": {
      "line-color": "rgb(115,62,136)",
      "line-width": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        3,
        0.25
      ]
    }
  },
  "region-fill": {
    "type": "fill",
    "paint": {
      "fill-color": "rgb(115,62,136)",
      "fill-opacity": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        0,
        0.1
      ]
    }
  },
  "region-label": {
    "type": "symbol",
    "layout": {
      "text-variable-anchor": [
        "center",
        "top"
      ],
      "text-radial-offset": 2,
      "text-field": "{name}",
      "text-font": [
        "Flexo Medium"
      ],
      "text-letter-spacing": 0.04
    },
    "paint": {
      "text-color": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        "#222222",
        "#000000"
      ],
      "text-halo-color": "hsl(0, 0%, 100%)",
      "text-halo-width": 0.85,
      "text-halo-blur": 1.5
    }
  },
  "air_traffic_control": {
    "type": "fill",
